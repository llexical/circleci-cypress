{
  "extends": "airbnb",
  "globals": {
    "cy": true,
    "Cypress": true,
    "describe": true,
    "it": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prefer-destructuring": ["warn", {
      "VariableDeclarator": {
        "array": true,
        "object": true
      }
    }]
  }
}
